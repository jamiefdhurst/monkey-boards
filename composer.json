{
    "name": "jamiefdhurst/monkey-boards",
    "description": "A basic PHP forum system built from the ground up to be small, lightweight, and robust.",
    "keywords": ["board", "bulletin-board", "discussion", "forum"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Hurst",
            "email": "jamie@jamiehurst.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/jamiefdhurst/monkey-boards/issues",
        "source": "https://github.com/jamiefdhurst/monkey-boards"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/http-foundation": "^4.1",
        "symfony/routing": "^4.1",
        "symfony/templating": "^4.1",
        "symfony/translation": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Monkey\\": "src/"
        }
    }
}
